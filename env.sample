# local dev server port, start local dev server by `npm run start`
PORT=6066
APP_HOST=localhost
APP_SERVER=http://localhost:6066
APP_SERVER_SECRET_KEY=secret_key_for_jwt_encode

# GOOGLE OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_ACCESS_TOKEN_URI=https://oauth2.googleapis.com/token
GOOGLE_AUTHORIZATION_URI=https://accounts.google.com/o/oauth2/v2/auth
GOOGLE_AUTH_SCOPES=https://www.googleapis.com/auth/forms.body.readonly,https://www.googleapis.com/auth/forms.responses.readonly,https://www.googleapis.com/auth/userinfo.profile
GOOGLE_AUTH_SCOPES_SEPARATOR=,
GOOGLE_PUBSUB_TOPIC_NAME=your_google_pub_sub_topic_name

# db uri
DATABASE_URL=sqlite://./db.sqlite
# client-side UI asset path
ASSETS_PATH=http://localhost:8081

# RingCentral Developer Portal
IM_SHARED_SECRET=
