    "serverless-deploy": "node ./scripts/serverless-deploy",
    "refresh-subscription": "node ./scripts/refresh-subscription",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "npm run client-build && npm run serverless-build",
    "deploy": "npm run serverless-deploy"
